{
	"private": true,
	"description": "BeyondJS AI Server",
	"name": "@beyond-js/ai-server",
	"repository": "https://github.com/beyondjs/engine",
	"dependencies": {
		"@beyond-js/kernel": "~0.1.9",
		"dotenv": "~16.3.1",
		"express": "4.18.2",
		"express-openapi-validator": "5.0.4"
	},
	"devDependencies": {
		"@beyond-js/local": "~0.1.3",
		"@beyond-js/bee": "^0.0.6",
		"@types/express": "^4.17.17",
		"@types/node": "~20.14.5"
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "node",
				"platform": "node",
				"ports": {
